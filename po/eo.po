msgstr "aŭtomate apliki pikselajn laborfluajn defaŭltojn"

#: ../build/bin/preferences_gen.h:5141
msgctxt "preferences"
msgid "scene-referred"
msgstr "scen-referita"

#: ../build/bin/preferences_gen.h:5146 ../build/bin/preferences_gen.h:5170
msgctxt "preferences"
